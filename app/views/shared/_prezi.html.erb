<section id="prezi-player">

<% width="width=20em"; height="height=10em" %>
<object id='prezi'>
		<% prezi = copywriting('Front Page Presentation', scope: 'body', phrase_type: 'text', default: '<p>Insert presentation material here. DO NOT LEAVE BLANK!</p>') %>
		
		<% begin %>
				<%= raw /<param.*<\/embed>/.match(prezi)[0].gsub(/width="\d*"/, width).gsub(/height="\d*"/, height) %>
		<% rescue %>
		<% end %>
</object>
</section>
